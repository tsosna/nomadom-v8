		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tailwindcss/typography": "^0.5.15",
		"@types/eslint": "^8.56.12",
		"@types/three": "^0.169.0",
		"autoprefixer": "^10.4.20",
		"bits-ui": "1.0.0-next.32",
		"clsx": "^2.1.1",
		"eslint": "^9.14.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"globals": "^15.11.0",
		"lucide-svelte": "^0.399.0",
		"postcss": "^8.4.47",
